
GPIO1   .EQU 8000H
PORT0   .EQU 8001H
PORT1   .EQU 8002H

	.ORG 200H
       
MORSE    JSR TONE
         LDA PORT0
         AND #%01000000
	 BEQ MORSE

	 JSR DELAY

WAIT	 LDA PORT0
	 AND #%01000000
	 BNE WAIT

	 JSR DELAY
	 JMP MORSE

TONE	 LDA #%00111111
         STA PORT1
	 JSR DELAY
	 LDA #%10111111
	 STA PORT1
	 JSR DELAY
	 JMP TONE

DELAY    LDY #$79
DELAY2   DEY
	 BNE DELAY2
	 RTS
         
	 .END
